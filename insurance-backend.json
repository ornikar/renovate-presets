  "masterIssue": true,
  "masterIssueApproval": true,
  "reviewers": [
    "team:insurance-backend"
  ],
  "labels": [
    "dependencies"
  ],
  "lockFileMaintenance": {
    "enabled": true,
    "schedule": []
